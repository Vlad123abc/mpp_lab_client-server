<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.gui.LoginController">
   <children>
      <Label layoutX="54.0" layoutY="72.0" text="Username:" />
      <TextField fx:id="textField" layoutX="151.0" layoutY="68.0" />
      <Label layoutX="56.0" layoutY="124.0" text="Password:" />
      <PasswordField fx:id="passwordField" layoutX="151.0" layoutY="120.0" />
      <Button layoutX="254.0" layoutY="175.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <Label fx:id="incorrectLabel" layoutX="126.0" layoutY="216.0" text="Incorrect username or password!" textFill="RED" />
   </children>
</AnchorPane>
